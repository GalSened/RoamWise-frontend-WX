# Environment variables for the traveling app
VITE_APP_NAME="RoamWise"
VITE_APP_VERSION="2.0.0"
VITE_UPDATE_CHECK_URL="https://api.roamwise.com/version"
VITE_ANALYTICS_ENDPOINT="https://analytics.roamwise.com/events"
VITE_ROUTING_PROVIDER="google"

# API Keys - KEEP SECURE, DO NOT COMMIT TO VERSION CONTROL
VITE_GOOGLE_MAPS_API_KEY="your_google_maps_api_key_here"
